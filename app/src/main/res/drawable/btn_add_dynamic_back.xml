<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/btn_add_selected"/>
            <corners android:radius="@dimen/btn_add_bg_radius_green"/>
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="@dimen/add_btn_stroke_width_purple" android:color="@color/btn_add_normal"/>
            <solid android:color="@android:color/transparent"/>
            <corners android:radius="@dimen/btn_add_bg_radius_purple"/>
        </shape>
    </item>
</selector>